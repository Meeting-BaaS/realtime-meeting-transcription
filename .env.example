# API Keys
MEETING_BAAS_API_KEY=your_meetingbaas_api_key
GLADIA_API_KEY=your_gladia_api_key

# Proxy Configuration
PROXY_HOST=0.0.0.0
PROXY_PORT=3000

# Audio Recording Configuration (optional)
# Enable audio recording to save meeting audio to WAV files
ENABLE_AUDIO_RECORDING=false
# Directory where audio recordings will be saved
AUDIO_OUTPUT_DIR=./recordings

# MeetingBaas API URL (optional)
# For production (default): https://api.meetingbaas.com
# For pre-production: https://api.pre-prod-meetingbaas.com
MEETING_BAAS_API_URL=https://api.meetingbaas.com
